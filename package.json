      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "gh-pages": "^6.0.0",
